package day6;

public class Main {
    
    public static void main(String[] args) {
        System.out.println("penggunaan tipe data pecahan");
        
        float U = 12.3f;
        System.out.println(U);
        //float dapat menyimpan sekitar 6-7 digit desimal
        
        double i = 45.6;
        System.out.println(i);
        //double dapat menyimpan sekitar 15-16 digit desimal
    } 
    
}
